<compliance xmlns="http://tail-f.com/ns/ncs">
    <template>
        <name>disable_propogate_ttl</name>

        <ned-id>
            <id xmlns:cisco-iosxr-cli-7.61="http://tail-f.com/ns/ned-id/cisco-iosxr-cli-7.61">
                cisco-iosxr-cli-7.61:cisco-iosxr-cli-7.61</id>
            <config>

                <mpls xmlns="http://tail-f.com/ned/cisco-ios-xr">
                    <ip-ttl-propagate>
                        <disable />
                    </ip-ttl-propagate>
                </mpls>

            </config>
        </ned-id>

        <ned-id>
            <id xmlns:cisco-ios-cli-6.108="http://tail-f.com/ns/ned-id/cisco-ios-cli-6.108">
                cisco-ios-cli-6.108:cisco-ios-cli-6.108</id>
            <config>

                <mpls xmlns="urn:ios">
                    <mpls-ip-conf>
                        <ip>
                            <propagate-ttl-conf>
                                <propagate-ttl>false</propagate-ttl>
                            </propagate-ttl-conf>
                        </ip>
                    </mpls-ip-conf>
                </mpls>

            </config>
        </ned-id>

    </template>
</compliance>